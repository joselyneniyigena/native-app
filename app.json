{
  "expo": {
    "name": "nativeApp",
    "slug": "nativeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.nativeApp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "plugin": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(native-app-main) to access your camera."
        }
      ]
    ],
    "scheme": "MyGoogleApp",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1e655ef6-18e3-4a8b-9001-64f857e04b99"
      }
    }
  }
}
